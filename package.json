  "scripts": {
    "dev": "set NEXT_PUBLIC_USERTYPE=EMP&& next dev",
    "teacherdev": "set NEXT_PUBLIC_USERTYPE=TCH&& next dev",
    "studentdev": "set NEXT_PUBLIC_USERTYPE=STD&& next dev",
    "proddev": "firebase use production&& set NEXT_PUBLIC_USERTYPE=EMP&& set NEXT_PUBLIC_ENVTYPE=prod&& next dev",
    "employeefirebasedeploy": "firebase use production && set NEXT_PUBLIC_ENVTYPE=prod&&  set NEXT_PUBLIC_USERTYPE=EMP&& next build&& next export&& firebase deploy --only hosting:employee",
    "studentfirebasedeploy": "firebase use production&& set NEXT_PUBLIC_ENVTYPE=prod&& set NEXT_PUBLIC_USERTYPE=STD&& next build&& next export&& firebase deploy --only hosting:student",
    "teacherfirebasedeploy": "firebase use production&& set NEXT_PUBLIC_ENVTYPE=prod&& set NEXT_PUBLIC_USERTYPE=TCH&& next build&& next export&& firebase deploy --only hosting:teacher",
    "dev-employeefirebasedeploy": "firebase use development&& set NEXT_PUBLIC_ENVTYPE=dev&&  set NEXT_PUBLIC_USERTYPE=EMP&& next build&& next export&& firebase deploy --only hosting:employee",
    "dev-studentfirebasedeploy": "firebase use development&& set NEXT_PUBLIC_ENVTYPE=dev&& set NEXT_PUBLIC_USERTYPE=STD&& next build&& next export&& firebase deploy --only hosting:student",
    "dev-teacherfirebasedeploy": "firebase use development&& set NEXT_PUBLIC_ENVTYPE=dev&& set NEXT_PUBLIC_USERTYPE=TCH&& next build&& next export&& firebase deploy --only hosting:teacher",
    "dev-devfirebasedeploy": "firebase use development&& set NEXT_PUBLIC_ENVTYPE=dev&& next build&& next export&& firebase deploy --only hosting",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --write .",
    "proddeployall": "npm run employeefirebasedeploy&& npm run studentfirebasedeploy&& npm run teacherfirebasedeploy",
    "devdeployall": "npm run dev-employeefirebasedeploy&& npm run dev-studentfirebasedeploy&& npm run dev-teacherfirebasedeploy"
  },
